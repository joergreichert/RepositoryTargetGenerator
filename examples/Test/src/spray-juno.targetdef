TargetDefinition "Spray Target Platform - Eclipse 4.2" 

	targetFile = spray-juno

	generate: target, categoryXML
	
	target configuration {
		includeMode = feature
		includeAllPlatforms = false
		includeSources = false
		includeConfigurePhase = true
		locationIncludeMode = planner
		type = InstallableUnit
	} 	
	
	categories {
		"3rdparty"
	}

	launcherArgs {
		vmArgs = "-Xmx512m", "-XX:MaxPermSize=128m"
		programArgs = "Name"
	}
	
	environment {
		nl = en
	}
